<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 修改此文件时，请注意SQL字段五个一段, 以提高可维护性 -->

<mapper namespace="com.zel.dbmanager.mapper.NoteMapper" >


    <select id="findAll" resultType="com.zel.pojo.entity.Note">
        SELECT * FROM tb_note t1
        ORDER BY
            t1.update_time DESC
    </select>

</mapper>